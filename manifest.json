{
  "name": "New Tab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "New tab page with changing backgrounds and news feed.",
  "permissions": ["storage", "bookmarks"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png"
  }
}